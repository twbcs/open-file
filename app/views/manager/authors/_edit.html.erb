<h1>修改<%= url_name(params[:controller].split('/').last) %></h1>
<i class="fas fa-times index-times"></i>
<div class="row">
  <div class="col-12">
    <%= form_with(model: [:manager, author]) do |form| %>
      <%= render 'form', form: form, author: author, all_names: all_names %>
    <% end %>
    <%= link_to manager_author_path(author), remote: true, class: "btn btn-primary" do %>
      <i class="fas fa-eye"></i> 檢視
    <% end%>
  </div>
</div>
